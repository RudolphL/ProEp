<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_ILogin">
                    <security mode="Message">
                        <message clientCredentialType="Windows" />
                    </security>
                </binding>
                <binding name="WSDualHttpBinding_IPost" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ProEpService/Service1/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ILogin"
                contract="ServiceReferenceApplication.ILogin" name="WSDualHttpBinding_ILogin">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ProEpService/Service1/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IPost"
                contract="ServiceReferenceApplication.IPost" name="WSDualHttpBinding_IPost">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>
